    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch server with memory db",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "env": {
                "CGO_CFLAGS":"-O1 -g"
            },
            "envFile": "${workspaceFolder}/.local/.memory.env",
            "program": "${workspaceFolder}/cmd/qe-server",
            "buildFlags": ["-tags", "duckdb_arrow"]
        },
        {
            "name": "Launch server with db",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "env": {
                "CGO_CFLAGS":"-O1 -g"
            },
            "envFile": "${workspaceFolder}/.local/.env",
            "program": "${workspaceFolder}/cmd/qe-server",
            "buildFlags": ["-tags=duckdb_arrow"]
        },
        {
            "name": "Launch server with auth and db",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "env": {
                "CGO_CFLAGS":"-O1 -g"
            },
            "envFile": "${workspaceFolder}/.local/.auth.env",
            "program": "${workspaceFolder}/cmd/qe-server",
            "buildFlags": ["-tags=duckdb_arrow"]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        }
    ]
}